<div id="header_sign_in_out">
	<% if user_signed_in? %>
		Signed in as <%= link_to current_user.email, edit_user_registration_path %>. Not you?
		<%= link_to "Sign out", destroy_user_session_path %>
	<% else %>
		<%= link_to "Sign in", new_user_session_path %> or <%= link_to "Register", new_user_registration_path %>
	<% end %>
</div>
